    "import numpy as np\n",
    "\n",
    "# Gravidade\n",
    "g = 9.8\n",
    "\n",
    "# Massa\n",
    "m = 75\n",
    "\n",
    "# Area, rÃ³, u e Cres\n",
    "A = 0.3\n",
    "Ro = 1.225\n",
    "u = 0.004\n",
    "Cres = 0.9\n",
    "\n",
    "\n",
    "# Velocidade inicial\n",
    "v = 30 * 1000 / 3600\n",
    "\n",
    "P = np.round((v * u * m * g) + (Cres * A * Ro * (v ** 2) * v) / 2, 4)\n",
    "print(\"P (40km/h) >\", P)"
